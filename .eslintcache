       <S.Empty src={Empty} alt=\"Lista vazia\" />\n          ) : (\n            <S.GridFavorite>\n              {listFavorite.map((item) => {\n                return (\n                  <S.CardInGrid\n                    key={item._id}\n                    onDoubleClick={(e) => removeFavorite(e, item._id)}\n                    src={item.card_images}\n                    alt=\"Card\"\n                  />\n                );\n              })}\n            </S.GridFavorite>\n          )}\n        </S.Wishlist>\n      </S.Container>\n    </Fragment>\n  );\n};\n\nexport default Favorite;\n","/home/andoly/Development/Project/React2021/yugioh_auth/src/components/Favorite/styles.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/Pages/Favorite/Favotire.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/components/Base/Base.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/Pages/Home/Home.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/components/Home/Home.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/components/Home/styles.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/components/SliderCards/SliderCards.js",["146"],"/home/andoly/Development/Project/React2021/yugioh_auth/src/components/Slider/Slider.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/components/SliderCards/styles.js",[],"/home/andoly/Development/Project/React2021/yugioh_auth/src/components/Slider/styles.js",[],{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","replacedBy":"150"},{"ruleId":"147","replacedBy":"151"},{"ruleId":"149","replacedBy":"152"},{"ruleId":"153","severity":1,"message":"154","line":35,"column":5,"nodeType":"155","endLine":35,"endColumn":7,"suggestions":"156"},{"ruleId":"157","severity":1,"message":"158","line":26,"column":9,"nodeType":"159","messageId":"160","endLine":26,"endColumn":17},"no-native-reassign",["161"],"no-negated-in-lhs",["162"],["161"],["162"],"re